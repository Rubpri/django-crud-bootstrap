{
    "builds": [{
      "src": "Universidad/wsgi.py",
      "use": "@ardnt/vercel-python-wsgi",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9",
        "entrypoint": "Universidad/wsgi.py"
      }
    }],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "Universidad/wsgi.py"
      }
    ]
  }
  