{
    "version": 2,
    "builds": [
      {
        "src": "vercel_build_staticfiles.sh",
        "use": "python3"
      }
    ],
    "env": {
      "DEBUG": "False",
      "ALLOWED_HOSTS": [".vercel.app"]
    },
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/index.html"
      }
    ]
  }