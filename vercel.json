{
  "version": 2,
  "builds": [
    {
      "src": "Universidad/wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "runtime": "python3.9"
      }
    },
    {
    "src": "build.sh",
    "use": "@vercel/static-build",
    "config": {
      "distDir": "staticfiles_build"
    }
    }
  ],
  "routes": [
    {
    "src": "/static/(.*)",
    "dest": "/static/$1"   // cambiar por 1 en vez de l
    },
    {
      "src": "/(.*)",
      "dest": "Universidad/wsgi.py"
    }
  ]
}